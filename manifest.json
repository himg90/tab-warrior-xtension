{
    "manifest_version": 3,
    "name": "Auto Tab Group",
    "description": "Automatically group tabs by lineage",
    "version": "0.2",
    "action": {},
    "permissions": [
        "tabs",
        "history",
        "webNavigation",
        "tabGroups"
    ],
    "background": {
        "service_worker": "src/background.js",
        "type": "module"
    },
    "icons": {
        "16": "assets/icons/icon-16.png",
        "48": "assets/icons/icon-48.png",
        "128": "assets/icons/icon-128.png"
    }
  }