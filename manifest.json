{
    "manifest_version": 3,
    "name": "Tab Warrior",
    "description": "Automatically manage tabGroups",
    "version": "0.1.0",
    "action": {},
    "permissions": [
        "tabs",
        "webNavigation",
        "tabGroups"
    ],
    "background": {
        "service_worker": "src/background.js",
        "type": "module"
    },
    "icons": {
        "16": "assets/warrior2/icon16.png",
        "32": "assets/warrior2/icon32.png",
        "48": "assets/warrior2/icon48.png",
        "128": "assets/warrior2/icon128.png"
    }
  }